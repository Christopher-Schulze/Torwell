{
  "name": "torwell84-v2-ui",
  "version": "2.4.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "lint:a11y": "svelte-kit sync && svelte-check --output human-verbose",
    "lint": "npx svelte-kit sync && eslint src/routes/__tests__/root.snapshot.test.ts",
    "test": "vitest run",
    "test:unit": "vitest run --reporter=default",
    "test:ui-snapshots": "vitest run --config vitest.snapshot.config.ts --reporter=dot",
    "bench:playwright": "playwright test --config scripts/benchmarks/playwright-perf.config.ts",
    "backup:ui": "bash scripts/backup_ui.sh",
    "setup": "bun install && bun add -d @sveltejs/kit"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tauri-apps/cli": "1.6.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/svelte": "^5.2.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.44.0",
    "fake-indexeddb": "^6.0.1",
    "globals": "^16.4.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "typedoc": "^0.28.7",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vite-plugin-inspect": "11.3.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.5.0",
    "dexie": "^4.0.11",
    "lucide-svelte": "^0.294.0",
    "svelte-error-boundary": "^1.0.2",
    "tailwindcss-glassmorphism": "^0.1.0",
    "tauri-plugin-keychain": "^2.0.1"
  }
}
